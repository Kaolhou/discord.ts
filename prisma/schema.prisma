// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model logs {
  id        String    @id @default(uuid())
  date      DateTime  @default(now())
  isError   Boolean
  authorId  String?      
  command   String?
  error     Json?
//  options   JSON?
}
