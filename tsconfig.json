{
  "compilerOptions": {
    "allowJs": true,
    "removeComments": true,
    "outDir": "./dist",
    "target": "es2021",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "resolveJsonModule": true,
    "skipLibCheck": true
  }
}
